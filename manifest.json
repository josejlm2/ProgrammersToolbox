{
  "manifest_version": 2,
  
  "name": "Programmer's Box",
  "description": "Because a programmer's best tool is knowledge",
  "version": "1.0",
  
  "content_scripts": [
    {
    "matches": ["<all_urls>"],
    "js": ["js/jquery-2.0.2.min.js"]
    }
  ],
  
  "background": {
    "scripts": ["js/eventPage.js"],
    "persistent": false
  },
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  }
  
}